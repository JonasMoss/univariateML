\begin_body

\begin_layout Section
Beta Distribution
\end_layout

\begin_layout Standard
Likelihood:
\begin_inset Formula 
\[
\prod_{i=1}^{n}x^{\alpha-1}\left(1-x\right)^{\beta-1}/\textrm{B}\left(\alpha,\beta\right)
\]

\end_inset

Log-likelihood:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-n\log\left[\textrm{B}\left(\alpha,\beta\right)\right]+\left(\alpha-1\right)\sum_{i=1}^{n}\log\left(x_{i}\right)+\left(\beta-1\right)\sum_{i=1}^{n}\log\left(1-x_{i}\right)
\]

\end_inset


\end_layout

\begin_layout Section
Beta Prime Distribution
\end_layout

\begin_layout Standard
Likelihood:
\begin_inset Formula 
\[
\prod_{i=1}^{n}\frac{x_{i}^{\alpha-1}\left(1+x_{i}\right)^{\beta-1}}{\textrm{B}\left(\alpha,\beta\right)}
\]

\end_inset

Log-likelihood:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-n\left[\log\left[\textrm{B}\left(\alpha,\beta\right)\right]-\left(\alpha-1\right)\frac{1}{n}\sum_{i=1}^{n}\log\left(x_{i}\right)+\left(\alpha+\beta\right)\frac{1}{n}\sum_{i=1}^{n}\log\left(1+x_{i}\right)\right]
\]

\end_inset


\end_layout

\begin_layout Section
Cauchy Distribution
\end_layout

\begin_layout Standard
[[fill in]]
\end_layout

\begin_layout Section
Exponential Distribution
\end_layout

\begin_layout Standard
The likelihood is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\prod_{i=1}^{n}\lambda e^{-\lambda x_{i}}
\]

\end_inset

the log-likelihood is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n\log\lambda-\lambda\sum_{i=1}^{n}x_{i}
\]

\end_inset

so the MLE is 
\begin_inset Formula $\widehat{\lambda}^{-1}=\frac{1}{n}\sum_{i=1}^{n}x_{i}$
\end_inset

.
 Thus the likelihood at the maximum is 
\begin_inset Formula $n\left(\log\widehat{\lambda}-1\right)$
\end_inset

.
\end_layout

\begin_layout Section
Gamma Distribution
\end_layout

\begin_layout Standard
The likelihood is
\begin_inset Formula 
\[
\prod_{i=1}^{n}\frac{\beta^{\alpha}}{\Gamma\left(\alpha\right)}x_{i}^{\alpha-1}e^{-\beta x_{i}}
\]

\end_inset

And the log-likelihood is:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n\left[\alpha\log\beta-\log\Gamma\left(\alpha\right)+\left(\alpha-1\right)L-\beta M\right]
\]

\end_inset

Where 
\begin_inset Formula $L=\frac{1}{n}\sum_{i=1}^{n}\log\left(x_{i}\right)$
\end_inset

 and 
\begin_inset Formula $M=\frac{1}{n}\sum_{i=1}^{n}x$
\end_inset

.
\end_layout

\begin_layout Standard
[Derive one-dimensional optimization.]
\end_layout

\begin_layout Section
Gumbel Distribution
\end_layout

\begin_layout Standard
The likelihood is
\begin_inset Formula 
\[
\prod_{i=1}^{n}\frac{1}{\beta}e^{-\left(\frac{x_{i}-\mu}{\beta}+e^{-\frac{x_{i}-\mu}{\beta}}\right)}
\]

\end_inset

And the log-likelihood is:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-n\left[\log\beta+\frac{1}{\beta}\left(M-\mu\right)+S\left(\mu,\beta\right)\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
M & = & \frac{1}{n}\sum_{i=1}^{n}x_{i}\\
S\left(\mu,\beta\right) & = & \frac{1}{n}\sum_{i=1}^{n}e^{-\frac{x_{i}-\mu}{\beta}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Section
Kumaraswamy Distribution
\end_layout

\begin_layout Standard
The likelihood is
\begin_inset Formula 
\[
\prod_{i=1}^{n}abx^{a-1}\left(1-x^{a}\right)^{b-1}
\]

\end_inset

And the log-likelihood is:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n\left[\log a+\log b+\left(a-1\right)M+\left(b-1\right)S\left(a\right)\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
M & = & \frac{1}{n}\sum_{i=1}^{n}\log x_{i}\\
S\left(a\right) & = & \frac{1}{n}\sum_{i=1}^{n}\log\left(1-x_{i}^{a}\right)
\end{eqnarray*}

\end_inset

Since the ML of 
\begin_inset Formula $b$
\end_inset

 is 
\begin_inset Formula $\widehat{b}=-S\left(a\right)^{-1}$
\end_inset

, the likelihood at the maximum is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n\left[\log a+\log b+\left(a-1\right)M-1+b^{-1}\right]
\]

\end_inset


\end_layout

\begin_layout Section
Laplace Distribution
\end_layout

\begin_layout Standard
The likelihood is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\prod_{i=1}^{n}\frac{1}{2\sigma}e^{-\frac{\left|x_{i}-\mu\right|}{\sigma}}
\]

\end_inset

the log-likelihood is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-n\left(\log2\sigma+\frac{1}{\sigma}\frac{1}{n}\sum_{i=1}^{n}\left|x_{i}-\mu\right|\right)
\]

\end_inset

At the maximum this equals
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-n\left(1+\log2\widehat{\sigma}\right)
\]

\end_inset


\end_layout

\begin_layout Section
Log-normal Distribution
\end_layout

\begin_layout Standard
The likelihood is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\prod_{i=1}^{n}\frac{1}{\sqrt{2\pi}\sigma x}e^{-\frac{\left(\log x_{i}-\mu\right)^{2}}{2\sigma^{2}}}
\]

\end_inset

the log-likelihood is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-\frac{n}{2}\left(\frac{1}{\sigma^{2}}\frac{1}{n}\sum_{i=1}^{n}\left(\log x_{i}-\mu\right)^{2}+\log2\pi\sigma+\frac{1}{n}\sum_{i=1}^{n}\log x_{i}\right)
\]

\end_inset

At the maximum this equals
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-\frac{n}{2}\left(1+\log2\pi\sigma+\mu\right)
\]

\end_inset


\end_layout

\begin_layout Section
Logistic Distribution
\end_layout

\begin_layout Section
Lomax Distribution
\end_layout

\begin_layout Subsection
Method of Moments
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\widehat{\alpha} & = & \frac{s^{2}-\overline{x}^{2}}{\frac{1}{2}s^{2}-\overline{x}^{2}}\\
\widehat{\lambda} & = & \overline{x}\left(\widehat{\alpha}-1\right)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsection
Likelihood
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\log\left(\frac{1}{n}\prod_{i=1}^{n}λκ\left(1+λx_{i}\right)^{-\left(\kappa+1\right)}\right)\\
\log\lambda+\log\kappa-\left(\kappa+1\right)S\left(\lambda\right)
\end{eqnarray*}

\end_inset

here 
\begin_inset Formula $S\left(\lambda\right)=\frac{1}{n}\sum_{i=1}^{n}\log\left(1+λx_{i}\right)$
\end_inset

.
 Since
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{d}{d\kappa}=\frac{1}{\kappa}-S\left(\lambda\right),
\]

\end_inset

the value of 
\begin_inset Formula $\kappa$
\end_inset

 is 
\begin_inset Formula $1/S\left(\lambda\right)$
\end_inset

 at the maximum.
 Thus
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\log\lambda+\log\kappa-\left(\kappa+1\right)S\left(\lambda\right) & =\\
\log\lambda-\log S\left(\lambda\right)-S\left(\lambda\right)-1
\end{eqnarray*}

\end_inset

Taking the derivative with respect to 
\begin_inset Formula $\lambda$
\end_inset

 yields:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{1}{\lambda}-\frac{S'\left(\lambda\right)}{S\left(\lambda\right)}-S'\left(\lambda\right)
\]

\end_inset

And the second derivative is:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-\frac{1}{\lambda^{2}}-\frac{S\left(\lambda\right)S''\left(\lambda\right)-S'\left(\lambda\right)^{2}}{S\left(\lambda\right)^{2}}-S''\left(\lambda\right)
\]

\end_inset


\end_layout

\begin_layout Standard
.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
S'\left(\lambda\right) & = & \frac{1}{n}\sum_{i=1}^{n}\frac{x_{i}}{1+\lambda x_{i}}\\
S''\left(\lambda\right) & = & -\frac{1}{n}\sum_{i=1}^{n}\frac{x_{i}^{2}}{\left(1+\lambda x_{i}\right)^{2}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\frac{l'\left(\lambda\right)}{l''\left(\lambda\right)} & = & \frac{S\left(\lambda\right)\lambda^{-1}-S'\left(\lambda\right)-S\left(\lambda\right)S'\left(\lambda\right)}{S\left(\lambda\right)\left(-\frac{1}{\lambda^{2}}-\frac{S\left(\lambda\right)S''\left(\lambda\right)-S'\left(\lambda\right)^{2}}{S\left(\lambda\right)^{2}}-S''\left(\lambda\right)\right)}\\
 & = & S\left(\lambda\right)^{2}\frac{S\left(\lambda\right)\lambda-\lambda^{2}S'\left(\lambda\right)-\lambda^{2}S\left(\lambda\right)S'\left(\lambda\right)}{S\left(\lambda\right)\left(-S\left(\lambda\right)^{2}-S\left(\lambda\right)S''\left(\lambda\right)-S'\left(\lambda\right)^{2}-S\left(\lambda\right)^{2}S''\left(\lambda\right)\right)}\\
 &  & S\left(\lambda\right)\frac{S\left(\lambda\right)\lambda-\lambda^{2}S'\left(\lambda\right)-\lambda^{2}S\left(\lambda\right)S'\left(\lambda\right)}{\left(-S\left(\lambda\right)^{2}-\lambda^{2}S\left(\lambda\right)S''\left(\lambda\right)-S'\left(\lambda\right)^{2}-\lambda^{2}S\left(\lambda\right)^{2}S''\left(\lambda\right)\right)}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
.
\end_layout

\begin_layout Standard
.
\end_layout

\begin_layout Standard
.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\frac{d}{d\lambda} & = & \frac{n}{\lambda}-\left(\kappa+1\right)\sum_{i=1}^{n}\frac{x_{i}}{1+\lambda x_{i}}\\
\frac{d}{d\kappa} & = & \frac{n}{\kappa}-\sum_{i=1}^{n}\log\left(1+λx_{i}\right)
\end{eqnarray*}

\end_inset

Thus 
\begin_inset Formula $\kappa\left(\lambda\right)=\left[\frac{1}{n}\sum_{i=1}^{n}\log\left(1+λx_{i}\right)\right]^{-1}$
\end_inset

.
 Hence
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
n\log\lambda+n\log\kappa-\left(\kappa+1\right)\sum_{i=1}^{n}\log\left(1+λx_{i}\right)\\
n\log\lambda-n\log\left[\frac{1}{n}\sum_{i=1}^{n}\log\left(1+λx_{i}\right)\right]-n-\sum_{i=1}^{n}\log\left(1+λx_{i}\right)\\
\log\lambda-\log\left[\frac{1}{n}\sum_{i=1}^{n}\log\left(1+λx_{i}\right)\right]-1-\frac{1}{n}\sum_{i=1}^{n}\log\left(1+λx_{i}\right)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{d}{d\lambda}=\frac{n}{\lambda}-\frac{\sum_{i=1}^{n}\frac{x_{i}}{1+\lambda x_{i}}}{\frac{1}{n}\sum_{i=1}^{n}\log\left(1+λx_{i}\right)}-\sum_{i=1}^{n}\frac{x_{i}}{1+\lambda x_{i}}
\]

\end_inset

This is zero when 
\begin_inset Formula 
\[
\frac{1}{\lambda}-\frac{\frac{1}{n}\sum_{i=1}^{n}\frac{x_{i}}{1+\lambda x_{i}}}{\frac{1}{n}\sum_{i=1}^{n}\log\left(1+λx_{i}\right)}-\frac{1}{n}\sum_{i=1}^{n}\frac{x_{i}}{1+\lambda x_{i}}
\]

\end_inset

This requires sums as 
\begin_inset Formula $3N$
\end_inset

 each iteration.
 One more
\end_layout

\begin_layout Subsection
Analysis
\end_layout

\begin_layout Standard
As 
\begin_inset Formula $\lambda\to0$
\end_inset

, 
\begin_inset Formula $S\left(\lambda\right)\to0$
\end_inset

 and 
\begin_inset Formula $\kappa\to\infty$
\end_inset

.
 By Taylor, 
\begin_inset Formula $\frac{1}{n}\sum_{i=1}^{n}\log\left(1+λx_{i}\right)\approx\frac{λ}{n}\sum_{i=1}^{n}x_{i}=\lambda\overline{x}$
\end_inset

 when 
\begin_inset Formula $\lambda\to0$
\end_inset

, hence 
\begin_inset Formula $\kappa\left(\lambda\right)\to\lambda^{-1}\overline{x}^{-1}$
\end_inset

.The density 
\begin_inset Formula $λκ\left(1+λx\right)^{-\left(\kappa+1\right)}$
\end_inset

 is approximately equal to 
\begin_inset Formula $λκ\left(1+λx\right)^{-\left(\lambda^{-1}\overline{x}^{-1}+1\right)}$
\end_inset

 when 
\begin_inset Formula $\lambda$
\end_inset

 is small enough.
 Since 
\begin_inset Formula $λκ\left(1+λx\right)^{-\left(\lambda^{-1}\overline{x}^{-1}+1\right)}\to\overline{x}^{-1}e^{-\overline{x}^{-1}x}$
\end_inset

, the maximum likelihood estimator converges to an exponential density.
\end_layout

\begin_layout Subsubsection
Concavity
\end_layout

\begin_layout Standard
The second derivative:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
-\frac{1}{\lambda^{2}}-\frac{S\left(\lambda\right)S''\left(\lambda\right)-S'\left(\lambda\right)^{2}}{S\left(\lambda\right)^{2}}-S''\left(\lambda\right)\\
-\frac{1}{\lambda^{2}}-S''\left(\lambda\right)\left(1+\frac{1}{S\left(\lambda\right)}\right)+\left(\frac{S'\left(\lambda\right)}{S\left(\lambda\right)}\right)^{2}
\end{eqnarray*}

\end_inset

Here 
\begin_inset Formula $-S''\left(\lambda\right)\left(1+\frac{1}{S\left(\lambda\right)}\right)$
\end_inset

 and 
\begin_inset Formula $\left(\frac{S'\left(\lambda\right)}{S\left(\lambda\right)}\right)^{2}$
\end_inset

 are always positive.
 
\begin_inset Formula $l''$
\end_inset

 is positive when
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-S''\left(\lambda\right)\left(1+\frac{1}{S\left(\lambda\right)}\right)+\left(\frac{S'\left(\lambda\right)}{S\left(\lambda\right)}\right)^{2}\geq\frac{1}{\lambda^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
S\left(\lambda\right) & = & \frac{1}{n}\sum_{i=1}^{n}\log\left(1+λx_{i}\right)\\
S'\left(\lambda\right) & = & \frac{1}{n}\sum_{i=1}^{n}\frac{x_{i}}{1+\lambda x_{i}}\\
S''\left(\lambda\right) & = & -\frac{1}{n}\sum_{i=1}^{n}\frac{x_{i}^{2}}{\left(1+\lambda x_{i}\right)^{2}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
1.) Show it has a global minimum.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\frac{1}{\lambda}-\frac{S'\left(\lambda\right)}{S\left(\lambda\right)}-S'\left(\lambda\right) & = & 0\\
 & \Updownarrow\\
1 & = & \lambda S'\left(\lambda\right)\left(1+\frac{1}{S\left(\lambda\right)}\right)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
2.) (Maybe first for one 
\begin_inset Formula $x$
\end_inset

)
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
S\left(\lambda\right) & = & \log\left(1+λx\right)\\
S'\left(\lambda\right) & = & \frac{x}{1+\lambda x}\\
S''\left(\lambda\right) & = & -\frac{x^{2}}{\left(1+\lambda x\right)^{2}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
.
\end_layout

\begin_layout Standard
.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
-\frac{1}{\lambda^{2}}-\frac{S\left(\lambda\right)S''\left(\lambda\right)+S''\left(\lambda\right)}{S\left(\lambda\right)^{2}}-S''\left(\lambda\right)\\
-\frac{1}{\lambda^{2}}+\frac{x^{2}}{\left(1+\lambda x\right)^{2}}\left(1+\frac{1}{\log\left(1+λx\right)}+\frac{1}{\log\left(1+λx\right)^{2}}\right) & \leq & 0\\
\frac{\lambda^{2}x^{2}}{\left(1+\lambda x\right)^{2}}\left(1+\frac{1}{\log\left(1+λx\right)}+\frac{1}{\log\left(1+λx\right)^{2}}\right) & \leq & 1\\
\lambda^{2}x^{2}\left(\log\left(1+λx\right)^{2}+\log\left(1+λx\right)+1\right) & \leq & \left(1+\lambda x\right)^{2}\log\left(1+λx\right)^{2}\\
 & \leq & \left(\lambda^{2}x^{2}+2\lambda x+1\right)\log\left(1+λx\right)^{2}\\
\\
\lambda^{2}x^{2}\left(\log\left(1+λx\right)+1\right) & \leq & \left(2\lambda x+1\right)\log\left(1+λx\right)^{2}\\
\\
\\
\\
-\frac{1}{\lambda^{2}}-S''\left(\lambda\right)\left(1+\frac{1}{S\left(\lambda\right)}\right)+\left(\frac{S'\left(\lambda\right)}{S\left(\lambda\right)}\right)^{2}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
.
\end_layout

\begin_layout Standard
.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
1=\frac{\lambda}{1+\lambda x}\left(1+\frac{1}{\log\left(1+λ\right)}\right)
\]

\end_inset


\end_layout

\begin_layout Subsection
Limits
\end_layout

\begin_layout Standard
If 
\begin_inset Formula $\kappa=\frac{\mu}{\lambda}$
\end_inset

 and 
\begin_inset Formula $\lambda\to0$
\end_inset

, 
\begin_inset Formula $f\left(x;\lambda,\kappa\right)\to\mu e^{-\mu x}$
\end_inset

.
 All CIs for 
\begin_inset Formula $\lambda$
\end_inset

 must include an open interval arbitrarily close to 
\begin_inset Formula $0$
\end_inset

 with positive probability.
 With the inverse parameterization, all CIs must be of infinite length with
 positive probability.
\end_layout

\begin_layout Section
Logit-Normal Distribution
\end_layout

\begin_layout Section
Normal Distribution
\end_layout

\begin_layout Standard
The likelihood is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\prod_{i=1}^{n}\frac{1}{\sqrt{2\pi}\sigma}e^{-\frac{1}{2\sigma^{2}}\left(x_{i}-\mu\right)^{2}}
\]

\end_inset

The log-likelihood is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-\frac{n}{2}\left(\frac{1}{\sigma^{2}}\frac{1}{n}\sum_{i=1}^{n}\left(x_{i}-\mu\right)^{2}+\log2\pi+\log\sigma\right)
\]

\end_inset

At the maximum it equals
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-\frac{n}{2}\left(1+\log2\pi\sigma\right)
\]

\end_inset


\end_layout

\begin_layout Section
Pareto Distribution
\end_layout

\begin_layout Standard
The likelihood is 
\begin_inset Formula 
\[
\prod_{i=1}^{n}ab^{a}x_{i}^{-\left(a+1\right)}1_{x\geq b}
\]

\end_inset

Hence the log-likelihood is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
l\left(a,b\right)=n\log a+na\log b-\left(a+1\right)\sum_{i=1}^{n}\log x_{i}
\]

\end_inset

The likelihood is increasing in 
\begin_inset Formula $b$
\end_inset

 until 
\begin_inset Formula $b>x_{i}$
\end_inset

 for some 
\begin_inset Formula $x_{i}$
\end_inset

 when it is infinite.
 Thus 
\begin_inset Formula $\widehat{b}=\min x_{i}$
\end_inset

.
 The derivatives of 
\begin_inset Formula $l$
\end_inset

 with respect to 
\begin_inset Formula $a$
\end_inset

 is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\frac{dl\left(a,b\right)}{da} & = & \frac{n}{a}+n\log b-\sum_{i=1}^{n}\log x_{i}
\end{eqnarray*}

\end_inset

Thus, denoting 
\begin_inset Formula $G=\frac{1}{n}\sum_{i=1}^{n}\log\left(x_{i}\right)$
\end_inset


\begin_inset Formula 
\begin{eqnarray*}
a^{-1} & = & G-\log b
\end{eqnarray*}

\end_inset

At the maxium, the log-likelihood is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n\left(\log a+a\log b-\left(a+1\right)G\right)
\]

\end_inset


\end_layout

\begin_layout Section
Power Distribution
\end_layout

\begin_layout Standard
Likelihood:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\prod_{i=1}^{n}\frac{\beta}{\alpha^{\beta}}x_{i}^{\beta-1}1_{x\leq\alpha}
\]

\end_inset

Log-likelihood
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
l\left(\alpha,\beta\right)=n\log\beta-n\beta\log\alpha+\left(\beta-1\right)\sum_{i=1}^{n}\log x_{i}
\]

\end_inset

The mle for 
\begin_inset Formula $\alpha$
\end_inset

 is 
\begin_inset Formula $\textrm{max}x_{i}$
\end_inset

.
 Take the derivative with respect to 
\begin_inset Formula $\beta$
\end_inset

 and obtain, with 
\begin_inset Formula $G=\frac{1}{n}\sum_{i=1}^{n}\log x_{i}$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{dl\left(\alpha,\beta\right)}{d\beta}=\frac{n}{\beta}-n\log\alpha+nG
\]

\end_inset

Thus 
\begin_inset Formula 
\[
\beta^{-1}=\log\left(\textrm{max}x_{i}\right)-G
\]

\end_inset

At the maximum, the log-likelihood is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n\left(\log\beta-\beta\log\alpha+\left(\beta-1\right)G\right)
\]

\end_inset


\end_layout

\begin_layout Section
Rayleigh Distribution
\end_layout

\begin_layout Standard
The likelihood is
\begin_inset Formula 
\[
\prod_{i=1}^{n}\frac{x}{\sigma^{2}}e^{-\frac{x^{2}}{2\sigma^{2}}}
\]

\end_inset

And the log-likelihood is:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n\left[L-\log\sigma^{2}-\frac{1}{2\sigma^{2}}M\right]
\]

\end_inset

Where 
\begin_inset Formula $L=\frac{1}{n}\sum_{i=1}^{n}\log\left(x_{i}\right)$
\end_inset

 and 
\begin_inset Formula $M=\frac{1}{n}\sum_{i=1}^{n}x^{2}$
\end_inset

.
\end_layout

\begin_layout Standard
The ML is 
\begin_inset Formula $\widehat{\sigma^{2}}=\frac{1}{2}M$
\end_inset

, hence the likelihood at the maximum is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n\left[L-\log\frac{1}{2}M-1\right]
\]

\end_inset


\end_layout

\begin_layout Section
Inverse Gamma
\end_layout

\begin_layout Section
Inverse Gaussian
\end_layout

\begin_layout Standard
The likelihood is
\begin_inset Formula 
\[
\prod_{i=1}^{n}\sqrt{\frac{\lambda}{2\pi x^{3}}}\exp\left(-\frac{\lambda\left(x-\mu\right)^{2}}{2\mu^{2}x}\right)
\]

\end_inset

And the log-likelihood is:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n\left[-\frac{3}{2}L+\frac{1}{2}\log\lambda-\frac{1}{2}\log2\pi-\frac{\lambda}{2\mu^{2}}\frac{1}{n}\sum_{i=1}^{n}\frac{\left(x_{i}-\mu\right)^{2}}{x}\right]
\]

\end_inset

Where 
\begin_inset Formula $L=\frac{1}{n}\sum_{i=1}^{n}\log\left(x_{i}\right)$
\end_inset

.
 Since the ML of 
\begin_inset Formula $\mu$
\end_inset

 is the empirical mean, 
\begin_inset Formula $S=\frac{1}{n}\sum_{i=1}^{n}\frac{\left(x_{i}-\mu\right)^{2}}{x}=\frac{1}{n}\sum_{i=1}^{n}\frac{\left(x_{i}-\overline{x}\right)^{2}}{x}$
\end_inset

 at the maximum.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-\frac{n}{2}\left[3L-\log\lambda+\log2\pi+\frac{\lambda}{\mu^{2}}S\right]
\]

\end_inset


\end_layout

\begin_layout Section
Uniform Distribution
\end_layout

\begin_layout Section
Weibull Distribution
\end_layout

\begin_layout Standard
The likelihood is
\begin_inset Formula 
\[
\prod_{i=1}^{n}\frac{a}{b}\left(\frac{x}{b}\right)^{a-1}e^{-\left(x/b\right)^{a}}
\]

\end_inset

And the log-likelihood is:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n\left[\log a-\log b+\left(a-1\right)\left(G-\log b\right)-b^{-a}M^{a}\right]
\]

\end_inset

Where 
\begin_inset Formula $G=\frac{1}{n}\sum_{i=1}^{n}\log\left(x\right)$
\end_inset

 and 
\begin_inset Formula $M^{a}=\frac{1}{n}\sum_{i=1}^{n}x^{a}$
\end_inset

.
\end_layout

\begin_layout Standard
[Derive one-dimensional optimization.]
\end_layout

\end_body
